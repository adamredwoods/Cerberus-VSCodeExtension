{
	"comments": {
		"lineComment": "'",
		"blockComment": [ "#rem", "#end" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"]
	],
	"indentationRules": {
		"increaseIndentPattern": "^\\s*(\\b(else|method|then|do|select|function|class|if)\\b((?!\\b(end|until|then)\\b).)*)$",
		"decreaseIndentPattern": "^\\s*((\\b(elseif|else|end|until|endif)\\b))"
	}
}
